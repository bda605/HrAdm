@model CmsEditDto 

<script src="~/js/view/XpCms.js"></script>
<script type="text/javascript">
    $(function () {
        _me.init();
    });
</script>

@await Component.InvokeAsync("XgProgPath", new { names = new string[] { ViewBag.ProgName } })
<div class="xp-prog">
    <div id="divRead">
        <form id='rform' class='xg-form' style="margin-bottom:0;">
            <div class="row">
                @await Component.InvokeAsync("XiText", new { title = Model.Title, fid = "Title", inRow = true })
                @await Component.InvokeAsync("XgFindTbar")
            </div>
        </form>

        <div class='xg-btns-box'>
            @await Component.InvokeAsync("XgCreate")
        </div>
        <table id="table1" class="table table-bordered xg-table" cellspacing="0">
            <thead>
                <tr>
                    <th>@Model.Title</th>
                    <th>@Model.StartTime</th>
                    <th>@Model.EndTime</th>
                    <th>狀態</th>
                    <th>建立時間</th>
                    <th>維護</th>
                </tr>
            </thead>
            <tbody></tbody>
        </table>
    </div>

    <div id="divEdit" class="xg-hide">
        <partial name="~/Views/XpCms/Edit.cshtml" />
    </div>
</div>
